<div class="page-header">
  <div class="container">
    <div class="row">
      <div class="col-lg-12">
        <div class="inner-header">
          <h3><%= t("profile") %></h3>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="section bg-cyan">
  <div class="container">
    <div class="row">
      <%= render ("layouts/candidate_sidebar")%>
      <div class="col-lg-9 col-md-9 col-xs-12">
        <div class="job-alerts-item candidates">
          <h3 class="alerts-title"><%= t(".manage_skill") %></h3>
          <div id="new-skill">
            <%= link_to t(".new_skill"), new_skill_path, remote: true, class:"btn btn-common btn-sm" %>
          </div>
          <div id="skills">
            <%= render @skills%>
          </div>
        </div>
      </div>
    </div>
  </div>
</div>
